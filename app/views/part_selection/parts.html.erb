						<% elsif session[:part_type] == "Memory" %>
							<%= render :partial => "motherboard", :locals => {:moboObj => @parts[i], :num => i} %>
						<% elsif session[:part_type] == "Cases" %>
							<%= render :partial => "motherboard", :locals => {:moboObj => @parts[i], :num => i} %>
						<% else %>
							<%= render :partial => "motherboard", :locals => {:moboObj => @parts[i], :num => i} %>
						<% end %>
					<% end %>
				<% elsif session[:page_num] == session[:max] %>
