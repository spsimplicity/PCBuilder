<div id="Main">
	<div class="side_tab">
		<ul id="Sort_options" class="tab_options">
			<li id="Sort_tab" class="tab">
				<center><a class="sort_link tab_link" href="#">Sort Options</a></center>
			</li>
		</ul>
	</div>
	<div id="Back_cats">
		<center><%= link_to_remote "Back to Categories", :update => 'Main', :url => {:controller => :part_categories, :action => :current}, :class => "back_button" %></center>
	</div>
	<div id="Content">
		<div id="Parts">
			<div id="Part_nav">
				<%= link_to_remote "Prev", :update => 'Main', :url => {:controller => :part_selection, :action => :previous} %>
				<div id="Header">
					<h1 id="Part_heading" align="center"><%= session[:part_type] %></h1>
				</div>
				<%= link_to_remote "Next", :update => 'Main', :url => {:controller => :part_selection, :action => :next} %>
			</div>
			<div id="Parts_list">
				<% if session[:page_num] == 1 %>
					<% for i in (0..9) do %>
