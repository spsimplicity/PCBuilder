			<div id="Page_nav">
			<%= link_to_remote "Prev", :update => 'Main', :url => {:controller => :part_selection, :action => :previous} %>
				<div id="Pages">
					<h4 align="center">
						<% for i in (1..session[:max]) %>
						<%= link_to_remote i, :update => 'Main', :url => {:controller => :part_selection, :action => :jump, :num => i} %>
						<% end %>
					</h4>
				</div>
				<%= link_to_remote "Next", :update => 'Parts_list', :url => {:controller => :part_selection, :action => :next} %>
			</div>
		</div>
	</div>
	<div class="side_tab">
		<ul id="Selected_options" class="tab_options">
			<li id="Selected_tab" class="tab">
				<center><button class="selected_link tab_link" href="" onclick="$('Selected_parts').toggle()">Selected Parts</button></center>
